<html>
<body>
<script>

console.log("Display the numbers from 1 to 20.");
    for(var i = 1; i <= 20; i++) {
       console.log(i);
    }
console.log("display the even numbers from 1 to 20")
for(var i = 2; i<= 20; i=i+2) {
    console.log(i);
}
console.log("display the odd numbers from 1 to 20")
for(var i = 1; i<= 20; i=i+2) {
    console.log(i);
}
console.log("display the multiples of of 5 to 100")
for(var i = 5; i<= 100; i=i+5) {
    console.log(i);
} 
console.log("display the square numbers from 1 to 100")
    for(var i = 1; i<= 10; i=i+1){
        console.log(i*i)
    }
console.log("display the numbers from 1 to 20")
for(var i = 20; i>= 1; i--){
    console.log(i)
}
console.log("display the even numbers from 1 to 20")
for(var i = 20; i>= 1; i=i-2){
    console.log(i)
}
console.log("display the odd numbers from 1 to 20")
for(var i = 19; i>= 1; i=i-2){
    console.log(i)
}
console.log("display the multiples of 5 to 100")
for(var i = 100; i>= 5; i=i-5){
    console.log(i)
}
console.log("display the square numbers from 1 to 100")
for(var i = 10; i>= 1; i--){
    console.log(i*i)
}



    </script>

</body>





</html>
